<Window x:Class="TheGameOfLife.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheGameOfLife"
        mc:Ignorable="d"
        Title="MainWindow" Width="800" Height="640" Background="White">
    <Grid>
        <Image x:Name="screen" HorizontalAlignment="Right" VerticalAlignment="Top" Width="600" Height="600" Loaded="Screen_Loaded"/>
    </Grid>
</Window>
